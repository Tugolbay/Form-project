{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "types": ["vite/client", "vue"],
    "plugins": [{ "name": "@vue/compiler-sfc" }],
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.vue"],
  "exclude": ["node_modules"]
}
